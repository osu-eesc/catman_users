/**
 * Implements hook_ctools_plugin_api().
 */
function catman_users_ctools_plugin_api() {
  list($module, $api) = func_get_args();
  if ($module == "strongarm" && $api == "strongarm") {
    return array("version" => "1");
  }
}

/**
 * Implements hook_default_profile2_type().
 */
function catman_users_default_profile2_type() {
  $items = array();
  $items['eesc_profile'] = entity_import('profile2_type', '{
    "userCategory" : true,
    "userView" : true,
    "type" : "eesc_profile",
    "label" : "EESC profile",
    "weight" : "0",
    "data" : { "registration" : 1 },
    "rdf_mapping" : [  ]
  }');
  return $items;
}
